    "cache-manager": "^5.2.4",
    "cache-manager-redis-store": "^3.0.1",
    "chalk": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.7.4",
    "consul": "^2.0.1",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.9",
    "opossum": "^9.0.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "prom-client": "^15.1.3",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sharp": "^0.34.4",
    "socket.io": "^4.8.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.22.0"
